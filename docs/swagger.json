                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "errorMessage": {
                                            "type": "string"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/investors/{id}": {
            "get": {
                "description": "Retrieve investor",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "investors"
                ],
                "summary": "Retrieve investor",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Investor ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully retrieve investor",
                        "schema": {
                            "$ref": "#/definitions/response.InvestorRetrieveResponse"
                        }
                    },
                    "400": {
                        "description": "Bad request",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/base.ErrorResponse"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "errorMessage": {
                                            "type": "string"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "404": {
                        "description": "Not found",
                        "schema": {
                            "allOf": [
